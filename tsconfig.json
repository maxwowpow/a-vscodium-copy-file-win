{
  "compilerOptions": {
    "target": "ES2022",
    "module": "commonjs",
    "moduleResolution": "node",
	  "declaration": true,
    "outDir": "./dist",
    "esModuleInterop": true,
    // "downlevelIteration": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    // "strict": false
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist"]
}
